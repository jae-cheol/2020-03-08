package exam.code1Z0_808;

import java.io.IOException;

class CheckingAccount {
	public int amount;
	public CheckingAccount(int amount) {
		this.amount = amount;
	}
	public int getAmount() {
		return amount;
	}
	public void changeAmount(int x) {
		amount += x;
	}
}

