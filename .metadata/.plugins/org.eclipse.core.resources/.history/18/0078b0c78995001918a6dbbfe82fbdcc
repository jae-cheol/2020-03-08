
public class test {
	public static void main(String[] args){
		String[] strs = new String[2];
		int idx = 0;
		
		for(String s : strs) {
			System.out.println("sagd");
			strs[idx].concat(" element " + idx);
			System.out.println("sagd");
			idx++;
		}
		for(idx=0; idx<strs.length; idx++) {
			System.out.println(strs[idx]);
		}
	}
}