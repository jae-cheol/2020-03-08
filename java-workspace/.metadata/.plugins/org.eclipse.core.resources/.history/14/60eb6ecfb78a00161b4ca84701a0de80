package kr.co.bit.util;

import java.sql.Connection;
import java.sql.Statement;

public class JDBCClose {

	public void close(Connection conn, Statement stmt) {
		try {
			if (conn != null) {
				conn.close();
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
		try {
			if (stmt != null) {
				stmt.close();
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
