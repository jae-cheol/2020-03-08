import java.util.Scanner;
/*
 * 10보다 작은 수 앞에 입력으로 0 을 넣어준다는 의미는 아님.! 
 */
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String s = sc.next();
		
		if(Integer.parseInt(s) >= 0 && Integer.parseInt(s) <= 99) {
			if(Integer.parseInt(s) < 10) {
				s = "0" + s;
			} else {
				
			}
		}
	}
}