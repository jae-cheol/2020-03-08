import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();	// 테스트 케이스 갯수.
		
		int[][] nNum = { {1}
						, {2, 4, 8, 6}
						, {3, 9, 7, 1}
						, {4, 6}
						, {5}
						, {6}
						, {7, 9, 3, 1}
						, {8, 4, 2, 6}
						, {9, 1}
						, {0} };
		
		int[] numA = new int[num];
		int[] numB = new int[num];
		for(int i=0; i<num; i++) {
			numA[i] = sc.nextInt();
			numB[i] = sc.nextInt();
		}
		
		int var = 0;
		for(int i=0; i<nNum.length; i++) {	// 10.
			for(int j=0; j<nNum[i].length; j++) {	// nNum 갯수.
				if(numA[i] == 1 || numA[i] == 5 
				|| numA[i] == 6 || numA[i] == 10) {
					System.out.println(nNum[i][j]);
				} else if(numA[i] == 4 || numA[i] == 9) {
					var = numB[i] % 2;
					if(var == 0) {
						System.out.println(nNum[i][j]);
					} else {
						System.out.println(nNum[i][j-1]);
					}
				} else if(numA[i] == 2 || numA[i] == 3
					   || numA[i] == 7 || numA[i] == 8) {
					var = numB[i] % 4;
					if(var == 0) {
						System.out.println(nNum[i][j]);
					} else {
						System.out.println(nNum[i][j-1]);
					}
				}
			}
		}
	}
}