import java.math.BigDecimal;
import java.util.Arrays;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int num = sc.nextInt();	// 테스트 케이스 갯수.

		int[] num1 = {1};
		int[] num2 = {2, 4, 8, 6};
		int[] num3 = {3, 9, 7, 1};
		int[] num4 = {4, 6};
		int[] num5 = {5};
		int[] num6 = {6};
		int[] num7 = {7, 9, 3, 1};
		int[] num8 = {8, 4, 2, 6};
		int[] num9 = {9, 1};
		int[] num10 = {0};
		
		int[] numA = new int[num];
		int[] numB = new int[num];
		for(int i=0; i<num; i++) {
			numA[i] = sc.nextInt();
			numB[i] = sc.nextInt();
		}

		int[] numC = new int[num];
		int var = 0;
		for(int i=0; i<num; i++) {
			if(numA[i] == 1) {
				numC[i] = 1;
				System.out.println(numC[i]);
			} else if(numA[i] == 2) {
				var = numB[i] % 4;
				System.out.println(num2[var]);
			} else if(numA[i] == 3) {
				var = numB[i] % 4;
				System.out.println(num3[var]);
			} else if(numA[i] == 4) {
				var = numB[i] % 2;
				System.out.println(num4[var]);
			} else if(numA[i] == 5) {
				numC[i] = 5;
				System.out.println(numC[i]);
			} else if(numA[i] == 6) {
				numC[i] = 6;
				System.out.println(numC[i]);
			} else if(numA[i] == 7) {
				var = numB[i] % 4;
				System.out.println(num7[var]);
			} else if(numA[i] == 8) {
				var = numB[i] % 4;
				System.out.println(num8[var]);
			} else if(numA[i] == 9) {
				var = numB[i] % 2;
				System.out.println(num9[var]);
			} else if(numA[i] == 10) {
				numC[i] = 0;
				System.out.println(numC[i]);
			}
		}
	}
}